<odoo>
    <record id="ir_cron_compute_total_overtime" model="ir.cron">
        <field name="name">Compute Total Overtime</field>
        <field name="model_id" ref="model_hr_employee" />
        <field name="state">code</field>
        <field name="code">model.cron_compute_total_overtime()</field>
        <field name="interval_number">1</field>
